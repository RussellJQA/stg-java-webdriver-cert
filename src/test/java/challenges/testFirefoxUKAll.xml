<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="FirefoxUKAllTestSuite" parallel="none">
    <test name="FirefoxUKGoogleTest">
        <parameter name="browserType" value="firefox"/>
        <parameter name="url" value="https://www.google.co.uk/"/>
        <classes>
            <class name="challenges.challenge1"/>
        </classes>
    </test>
    <test name="FirefoxUKCopartTest1">
        <parameter name="browserType" value="firefox"/>
        <parameter name="url" value="https://www.copart.co.uk/"/>
        <classes>
            <class name="challenges.challenge2"/>
            <class name="challenges.challenge3"/>
        </classes>
    </test>
    <test name="FibonacciTest">
        <classes>
            <class name="challenges.challenge4"/>
        </classes>
    </test>
    <test name="FirefoxUKCopartTest2">
        <parameter name="browserType" value="firefox"/>
        <parameter name="url" value="https://www.copart.co.uk/"/>
        <classes>
            <class name="challenges.challenge5"/>
            <class name="challenges.challenge6"/>
            <class name="challenges.challenge7"/>
        </classes>
    </test>
</suite>